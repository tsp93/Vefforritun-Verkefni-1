    "concurrently": "4.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "node-sass": "4.10.0",
    "rollup": "0.67.0",
    "rollup-plugin-babel": "4.0.3",
    "stylelint": "9.7.1",
    "stylelint-config-primer": "2.2.11"
  },
  "dependencies": {
